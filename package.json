{
  "name": "bitjoe-js",
  "version": "0.1.0",
  "description": "Tradle bitjoe component in JS",
  "main": "index.js",
  "scripts": {
    "jsbeautify": "js-beautify -r {*.js,lib/*.js,lib/**/*.js,test/*.js} --config node_modules/tradle-style/.jsbeautifyrc",
    "jshint": "jshint {*.js,**/*.js} --config node_modules/tradle-style/.jshintrc --exclude-path=.gitignore",
    "jscs": "jscs {*.js,**/*.js} --config node_modules/tradle-style/.jscsrc",
    "test": "node test",
    "test-offline": "node test/offline",
    "test-online": "node test/basic && node test/endToEnd"
  },
  "pre-commit": [
    "jscs",
    "jshint",
    "test-offline"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tradle/bitkeeper-js.git"
  },
  "bugs": {
    "url": "https://github.com/tradle/bitkeeper-js/issues"
  },
  "homepage": "https://github.com/tradle/bitkeeper-js",
  "keywords": [
    "bitcoin",
    "blockchain",
    "tradenet",
    "tradle",
    "distributed",
    "storage",
    "decentralized",
    "storage"
  ],
  "license": "MIT",
  "dependencies": {
    "bip32-utils": "^0.2.1",
    "bip39": "^2.1.0",
    "bitcoinjs-lib": "^1.4.1",
    "bitkeeper-client-js": "git://github.com/tradle/bitkeeper-client-js.git",
    "bs58": "^2.0.0",
    "buffer-equal": "0.0.1",
    "cb-blockr": "^3.0.2",
    "cb-helloblock": "^0.4.7",
    "cb-wallet": "git://github.com/tradle/cb-wallet.git",
    "concat-stream": "^1.4.7",
    "create-torrent": "^3.5.0",
    "debug": "^2.1.1",
    "defaults": "^1.0.0",
    "dezalgo": "^1.0.1",
    "elliptic": "^0.15.15",
    "extend": "^2.0.0",
    "once": "^1.3.1",
    "parse-torrent": "^2.3.0",
    "q": "^1.1.2",
    "re-emitter": "https://registry.npmjs.org/re-emitter/-/re-emitter-1.0.0.tgz",
    "secure-random": "^1.1.1",
    "simple-jobs": "^0.0.1",
    "testnet-charger": "^0.0.2",
    "tradle-utils": "^0.1.0",
    "tx-walker": "0.0.1",
    "typeforce": "^0.1.0",
    "uniq": "^1.0.1"
  },
  "devDependencies": {
    "browserify-fs": "^1.0.0",
    "equals": "^1.0.0",
    "js-beautify": "^1.5.4",
    "jshint": "latest",
    "pre-commit": "0.0.11",
    "tape": "^3.0.3",
    "tradle-style": "latest",
    "which": "^1.0.8"
  }
}
